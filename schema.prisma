generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id     Int     @id @default(autoincrement())
  name   String
  price  Decimal
  orders Order[]
}

model Order {
  id            Int       @id @default(autoincrement())
  data          DateTime
  tracking_code String
  total         Decimal
  products      Product[]
}
