// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}


model Product{
  id Int @id @default(autoincrement())
  name String
  price Decimal
  orders Order[]
}

model Order {
  id Int @id @default(autoincrement())
  data DateTime
  products Product[]
  tracking_code String
  total Decimal
}